<script type="text/javascript">
    function Expand_Detail(Summary_DivId, Detail_DivId) {
        $(Summary_DivId).click(function () {
            var IsVisibleDiv = $(Detail_DivId).is(":visible");
            $(Detail_DivId).slideToggle();

            if (IsVisibleDiv == false)
            {
                $('html, body').animate({
                    scrollTop: $(Detail_DivId).offset().top
                }, 1000);
            }
        });
    }

    Expand_Detail('#VehicleSaleSummary', "#VehicleSaleDetail")
    Expand_Detail('#VehicleProfitSummary', "#VehicleProfitDetail")
    Expand_Detail('#DebtorsSummary', "#DebtorsDetail")
    Expand_Detail('#BankBalanceSummary', "#BankBalanceDetail")

    Expand_Detail('#VehicleStockSummary', "#VehicleStockDetail")
    Expand_Detail('#ExpenseSummary', "#ExpenseDetail")
    Expand_Detail('#CreditorsSummary', "#CreditorsDetail")
    Expand_Detail('#CashBalanceSummary', "#CashBalanceDetail")

    Expand_Detail('#VehicleSaleDetailClose', "#VehicleSaleDetail")
    Expand_Detail('#VehicleProfitDetailClose', "#VehicleProfitDetail")
    Expand_Detail('#DebtorsDetailClose', "#DebtorsDetail")
    Expand_Detail('#BankBalanceDetailClose', "#BankBalanceDetail")

    Expand_Detail('#VehicleStockDetailClose', "#VehicleStockDetail")
    Expand_Detail('#ExpenseDetailClose', "#ExpenseDetail")
    Expand_Detail('#CreditorsDetailClose', "#CreditorsDetail")
    Expand_Detail('#CashBalanceDetailClose', "#CashBalanceDetail")
</script>

